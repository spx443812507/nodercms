<div class="container">
    <div class="row">
        <div class="col-md-9">
            <ol class="breadcrumb">
                <li><a href="/">Home</a></li>
                <li class="active">Search</li>
            </ol>
            <div class="row">
                <div class="col-md-6">
                    <div class="panel panel-less">
                        <div class="panel-heading">
                            Search Result
                        </div>
                        <div class="list-group list-group-less">
                            <ins class="adsbygoogle"
                                 style="display:block"
                                 data-ad-format="fluid"
                                 data-ad-layout-key="-i2+e-f-4c+9b"
                                 data-ad-client="ca-pub-7524637416419319"
                                 data-ad-slot="5766598392"></ins>
                            <script>
                                (adsbygoogle = window.adsbygoogle || []).push({});
                            </script>
                            {{#foreach list.contents}}
                                <a href="{{url}}" class="list-group-item text-overflow">
                                    <div class="card-img">
                                        <img alt="img" src="{{thumbnail.src}}">
                                    </div>
                                    <div class="card-info">
                                        <section class="title">{{title}}</section>
                                        <section>
                                            <section class="social-bar">
                                                <i class="iconfont icon-eye"></i>
                                                <span>{{reading.total}}</span>
                                            </section>
                                        </section>
                                    </div>
                                </a>
                            {{/foreach}}
                        </div>
                    </div>
                </div>
            </div>
            <nav class="text-center">
                <ul class="pagination">
                    {{#foreach list.pagination}}
                        <li {{#if active}}class="active"{{/if}}>
                            <a href="{{url}}">{{name}}</a>
                        </li>
                    {{/foreach}}
                </ul>
            </nav>
        </div>
        {{> side-bar}}
    </div>
</div>
