<div class="game-title">
    <div class="flag"></div>
    <div class="title">热门游戏</div>
    <!--<a class="see-all" href="{{path}}">全部</a>-->
</div>
<div class="card-wrapper">
    {{#foreach readingList.total limit="5"}}
        <a class="card" href="{{href}}">
            <dl>
                <dt class="card-img">
                    <img alt="img" src="{{thumbnail.src}}">
                </dt>
                <dd class="card-content">
                    <span class="card-name">{{title}}</span>
                    <span class="tit-start">
                        <span>{{category.name}}</span>
                        <i class="iconfont icon-star"></i>
                        <span class="start">{{reading.total}}</span>
                    </span>
                </dd>
            </dl>
        </a>
    {{/foreach}}
</div>
{{#foreach lists}}
    <div class="game-title">
        <div class="flag"></div>
        <div class="title">{{name}}</div>
        <a class="see-all" href="{{path}}">全部</a>
    </div>
    <div class="card-wrapper">
        {{#foreach contents limit="5"}}
            <a class="card" href="{{href}}">
                <dl>
                    <dt class="card-img">
                        <img alt="img" src="{{thumbnail.src}}">
                    </dt>
                    <dd class="card-content">
                        <span class="card-name">{{title}}</span>
                        <span class="tit-start">
                            <span>{{category.name}}</span>
                            <i class="iconfont icon-star"></i>
                            <span class="start">{{reading.total}}</span>
                        </span>
                    </dd>
                </dl>
            </a>
        {{/foreach}}
    </div>
{{/foreach}}
